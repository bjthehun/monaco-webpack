{
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "./dist/",
		"target": "es6",
		"strict": true,
		"sourceMap": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "ES2022",
		"moduleResolution": "node",
		"lib": ["ESNext", "DOM", "WebWorker"]
	},
}
